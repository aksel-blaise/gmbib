@ARTICLE{SamperCarro2018124,
author={Samper Carro, S.C. and Louys, J. and O'Connor, S.},
title={Shape does matter: A geometric morphometric approach to shape variation in Indo-Pacific fish vertebrae for habitat identification},
journal={Journal of Archaeological Science},
year={2018},
volume={99},
pages={124-134},
doi={10.1016/j.jas.2018.09.010},
note={cited By 1},
