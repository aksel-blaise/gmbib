@ARTICLE{Castiñeira2011335,
author={Castiñeira, C. and Cardillo, M. and Charlin, J. and Baeza, J.},
title={Geometric morphometric analysis in fish tail points of Uruguay [Analisis de morfometría geométrica en puntas cola de pescado del uruguay]},
